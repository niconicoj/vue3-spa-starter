{
  "name": "vue3-spa-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibcheck && vite build",
    "build:production": "vue-tsc --noEmit --skipLibcheck && vite build --production",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest --environment jsdom",
    "test:coverage": "vitest run --environment jsdom --coverage",
    "test:e2e": "nightwatch",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@quasar/extras": "^1.15.6",
    "dayjs": "^1.11.6",
    "quasar": "^2.10.2",
    "roboto-fontface": "*",
    "vitest": "^0.25.3",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.0",
    "vuex": "^4.0.2",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^6.5.13",
    "@storybook/testing-vue3": "^0.0.2",
    "@testing-library/vue": "^6.6.1",
    "@quasar/vite-plugin": "^1.2.3",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.13",
    "@types/nightwatch": "^2.3.14",
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.4",
    "chromedriver": "^107.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "flush-promises": "^1.0.2",
    "geckodriver": "^3.2.0",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "nightwatch": "^2.5.1",
    "prettier": "2.8.0",
    "sass": "^1.32.12",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.0",
    "vite": "^3.0.9",
    "vite-plugin-nightwatch": "^0.3.2",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.9"
  }
}
